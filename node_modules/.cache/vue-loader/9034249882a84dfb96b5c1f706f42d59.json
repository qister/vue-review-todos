t default {\n  name: 'App',\n  data() {\n    return {\n      todos: [],\n      loading: true,\n      filter: 'all'\n    }\n  },\n  mounted() {\n    fetch('https://jsonplaceholder.typicode.com/todos?_limit=3')\n      .then(response => response.json())\n      .then(json => {\n        this.todos = json\n        this.loading = false\n        })\n  },\n  watch: { \n    filter(value) {\n      console.log(value);\n    }\n    },\n  methods: {\n    removeTodo(id) {\n      this.todos = this.todos.filter(todo => todo.id !== id)\n    },\n    addTodo(todo) {\n      this.todos.push(todo)\n    }\n  },\n  components: {\n    TodoList,\n    AddTodo,\n    Loader\n  }\n}\n</script>"]}]}